<% include ./partials/header %>

<div class="container">
    <div class="jumbotron" id="home-header">
        <h1 class="display-4">Welcome to Your Grove</h1>
    </div>
</div>

<div class="" id="slideshow">
    <div id="carouselIndicators" class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
            <li data-target="#carouselIndicators" data-slide-to="0" data-pause="hover" class="active"></li>
            <li data-target="#carouselIndicators" data-slide-to="1" data-pause="hover"></li>
            <li data-target="#carouselIndicators" data-slide-to="2" data-pause="hover"></li>
            <li data-target="#carouselIndicators" data-slide-to="3" data-pause="hover"></li>
            <li data-target="#carouselIndicators" data-slide-to="4" data-pause="hover"></li>
        </ol>
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img class="d-block h-100 w-100" src="images/relentless-church-616057-unsplash.jpg" alt="Baptism">
            </div>
            <div class="carousel-item">
                <img class="d-block h-100 w-100" src="/images/aaron-burden-287555-unsplash.jpg" alt="Bible">
            </div>
            <div class="carousel-item">
                <img class="d-block h-100 w-100" src="images/jeremy-perkins-253184-unsplash.jpg" alt="Praise">
            </div>
            <div class="carousel-item">
                <img class="d-block h-100 w-100" src="images/matt-botsford-566660-unsplash.jpg" alt="Bible Study">
            </div>
            <div class="carousel-item">
                <img class="d-block h-100 w-100" src="images/tim-marshall-76166-unsplash.jpg" alt="Faith">
            </div>
        </div>
        <a class="carousel-control-prev to-front" href="#carouselIndicators" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next to-front" href="#carouselIndicators" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
</div>

<div class="container-fluid" id="white-back">
    <section class="connect">
        <div class="container">
            <p>Come click a link to see the thing <a class="btn btn-outline-info" href="/about" role="button">here.</a></p>
        </div>
    </section>
</div> 

